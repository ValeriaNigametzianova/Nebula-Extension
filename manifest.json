{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Nebula",
    "description": "",
    "action":{
        "default_popup": "src/html/popup.html"
    },
    "content_scripts": [
        {
            "js": [
                "src/content.jsx"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "permissions":
    [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background":{
        "service_worker": "src/background.js"
    }
}