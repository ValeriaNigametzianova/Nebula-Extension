{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Nebula",
  "description": "",
  "icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48":"favicon48.png",
    "128":"favicon128.png"
  },
  "action": {
    "default_popup": "src/html/popup.html",
    "default_icon": {
      "16": "favicon16.png",
      "32": "favicon32.png",
      "48":"favicon48.png",
      "128":"favicon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.jsx"]
    }
  ],
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "src/background.jsx"
  }
}
